// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Exercise {
  id           Int      @id @default(autoincrement())
  name         String
  difficulty   Int
  description  String?
  hints        String[]
  functionName String?
  paramNames   String[]
  input        String?
  output       String?
  User         User?    @relation(fields: [userId], references: [id])
  userId       Int?
}

model User {
  id       Int        @id @default(autoincrement())
  email    String     @unique
  username String
  password String
  solved   Exercise[]
}
