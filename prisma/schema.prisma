// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test {
  id         Int      @id @default(autoincrement())
  testInput  String[]
  testOutput String[]
  exercise   Exercise @relation(fields: [exerciseId], references: [id])
  exerciseId Int      @unique // relation scalar field (used in the `@relation` attribute above)
}

model Exercise {
  id           Int      @id @default(autoincrement())
  name         String
  difficulty   Int
  category     String
  description  String?
  hints        String[]
  functionName String?
  paramNames   String[]
  input        String?
  output       String?
  test         Test?
}

model User {
  id     Int    @id @default(autoincrement())
  authId String @unique
  solved Int[]
}
